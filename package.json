{
  "name": "renku-jupyterlab-ts",
  "version": "0.1.2",
  "description": "A Renku extension for JupyterLab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts",
  "bugs": {
    "url": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts/issues"
  },
  "license": "Apache-2.0",
  "author": "Jiri Kuncar",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/apputils": "^0.18.4",
    "@jupyterlab/notebook": "^0.18.5",
    "@jupyterlab/terminal": "^0.18.4",
    "@phosphor/widgets": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.1",
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
