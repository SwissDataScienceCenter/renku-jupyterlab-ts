{
  "name": "renku-jupyterlab-ts",
  "version": "0.1.5",
  "description": "A Renku extension for JupyterLab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts",
  "bugs": {
    "url": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts/issues"
  },
  "license": "Apache-2.0",
  "author": "Jiri Kuncar",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SwissDataScienceCenter/renku-jupyterlab-ts.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "node_modules/eslint/bin/eslint.js src/*",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "@jupyterlab/notebook": "^0.19.2",
    "@jupyterlab/terminal": "^0.19.1",
    "@types/react-dom": "^16.4.2",
    "react-dom": "~16.4.0"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1",
    "typescript-eslint-parser": "^21.0.0"
  },
  "resolutions": {
    "@types/react": "^16.4.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
